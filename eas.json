{
  "cli": {
    "version": ">= 3.15.1",
    "appVersionSource": "local"
  },
  "build": {
    "production": {
      "developmentClient": false,
      "distribution": "store",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "ENV": "production"
      }
    },
    "preview": {
      "developmentClient": false,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "ENV": "preview"
      }
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "ENV": "development"
      }
    }
  },
  "submit": {
    "production": {},
    "preview": {}
  }
}
